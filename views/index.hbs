<h1>{{title}}</h1>
{{#each employees}}
    <div class="employee" id="{{this._id}}">
        <h2>{{this.name}}</h2>
        <p><span class="bold">Email:</span> {{this.email}}</p>
        
        {{#each department}}
            <p><span class="bold">Department:</span> {{this.dep_name}}</p>
        {{/each}}

        {{#each title}}
            <p><span class="bold">Job Title:</span> {{this.title_name}}</p>
        {{/each}}
    </div>
{{/each}}